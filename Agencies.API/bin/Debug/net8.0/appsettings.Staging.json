    "Audience": "AgenciesClient_Staging",
    "ExpiryMinutes": 60
  },
  "Cors": {
    "AllowedOrigins": [
      "https://agencies-staging.azurewebsites.net",
      "http://localhost:7149"
    ]
  },
  "Features": {
    "EnableSwagger": true,
    "EnableDetailedErrors": true,
    "EnableDatabaseLogging": false
  },
  "RateLimiting": {
    "EnableRateLimiting": true,
    "PermitLimit": 200,
    "Window": 1,
    "QueueLimit": 20
  }
